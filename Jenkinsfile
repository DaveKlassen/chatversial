pipeline {
  agent any
  stages {
    stage('Init') {
      steps {
        echo 'init'
        sh 'echo \'initializing...\''
      }
    }
    stage('Build') {
      steps {
        echo 'build'
        sh 'echo \'Building...\''
      }
    }
    stage('Deploy') {
      steps {
        echo 'deploy'
        sh 'echo \'Deploying...\''
      }
    }
  }
}